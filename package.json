{
  "name": "instabyte",
  "version": "1.0.0",
  "description": "An app that allows users upload images to share with friends and family.",
  "main": "index.js",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon index.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
